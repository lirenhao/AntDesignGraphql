{
  "private": true,
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "*.graphql",
      "*.json"
    ],
    "watch": [
      "data/",
      "graphql/"
    ]
  },
  "scripts": {
    "start": "nodemon --exec npm run run-start",
    "run-start": "node_modules/.bin/babel-node index.js"
  },
  "dependencies": {
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.2.1",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.4",
    "jsonfile": "^5.0.0",
    "moment": "^2.24.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.6.1",
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "nodemon": "^1.18.11"
  }
}
